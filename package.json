{
  "name": "ulbi-course",
  "private": true,
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "webpack serve --env PORT=3000 --env mode=development",
    "build:prod": "webpack --env mode=production",
    "build:dev": "webpack --env mode=development",
    "analyze": "cross-env analyze=1 webpack --env mode=production",
    "check-types": "tsc --pretty --noEmit ",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write . --config ./.prettierrc && npx stylelint \"**/*.scss\" --fix",
    "lint": "eslint --fix . --ext ts --ext tsx --ext js",
    "format-all": "npm run lint && npm run format",
    "test": "jest --config ./config/jest/jest.config.ts --watch",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:ui": "loki test --config ./config/storybook",
    "test:ui:ok": "loki approve --config ./config/storybook",
    "storybook": "start-storybook -p 6006 -c ./config/storybook",
    "build-storybook": "build-storybook -c ./config/storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
